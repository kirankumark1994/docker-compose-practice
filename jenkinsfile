pipeline {
    agent any
    
    
    stages {
        stage('checkout') {
            steps {
               git 'https://github.com/kirankumark1994/docker-compose-practice.git'
            }
        }
       
        stage('up') {
            steps {
                sh 'docker-compose up -d'
            }
        }
       
    }
    post {
  success {
    echo 'success'
  }
  failure {
    echo 'failure'
  }
}

}
